\begin_layout Standard
Se pide:
\end_layout

\begin_layout Enumerate
Determinar las salidas del sistema de inferencia borroso para un avión con
 una altitud de 700fts y una velocidad de 15 fts/s utilizando, el fuzzificador
 unitario y:
\end_layout

\begin_deeper
\begin_layout Enumerate
El operador implicación de Mandami
\end_layout

\begin_layout Enumerate
El operador de implicación de Larsen
\end_layout

\end_deeper
\begin_layout Enumerate
Defuzzificar el resultado utilizando la técnica del centroide
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mu_{h}=\left[\frac{1}{700}\right]
\]

\end_inset


\begin_inset Formula 
\[
\mu_{v}=\left[\frac{1}{15}\right]
\]

\end_inset


\begin_inset Formula 
\[
muy\ grande\left(MUY\negthickspace L\right)=\mu_{MUY\ L}\left(x\right)=\mu_{L}\left(x\right)^{2}=\left\{ \frac{0}{500}+\frac{0.04}{600}+\frac{0.16}{700}+\frac{0.36}{800}+\frac{0.64}{900}+\frac{1}{1000}\right\} 
\]

\end_inset


\begin_inset Formula 
\[
\mu_{MUY\ L}\left(x\right)=\mu_{L}\left(x\right)^{2}=\begin{cases}
0 & x<500\\
\left(\frac{x}{500}-1\right)^{2} & 500\leq x\leq1000
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
muy\ alta\ ascendente\left(MUY\ UL\right)=\mu_{MUY\ UL}\left(x\right)=\mu_{UL}\left(x\right)^{2}=\left\{ \frac{0}{10}+\frac{0.25}{15}+\frac{1}{20}+\frac{1}{25}+\frac{1}{30}\right\} 
\]

\end_inset


\begin_inset Formula 
\[
\mu_{MUY\ UL}\left(x\right)=\mu_{UL}\left(x\right)^{2}=\begin{cases}
0 & -30\leq x\leq10\\
\left(\frac{x}{10}-1\right)^{2} & 10<x\leq20\\
1 & 20<x\leq30
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
algo\ media\left(ALGO\ M\right)=\mu_{ALGO\ M}\left(x\right)=\mu_{M}\left(x\right)^{0.75}=\left\{ \frac{0}{300}+\frac{0.2990698}{400}+\frac{0.5029734}{500}+\frac{0.6817316}{600}+\frac{0.845897}{700}+\frac{1}{800}+\frac{0.845897}{900}+\frac{0.6817316}{1000}\right\} 
\]

\end_inset


\begin_inset Formula 
\[
\mu_{ALGO\ M}\left(x\right)=\mu_{M}\left(x\right)^{0.75}=\begin{cases}
0 & x<300\\
\left(\frac{x}{500}-\frac{3}{5}\right)^{0.75} & 300\leq x\leq800\\
\left(-\frac{x}{500}+\frac{13}{5}\right)^{0.75} & 800<x\leq1000
\end{cases}
\]

\end_inset


\begin_inset Formula 
