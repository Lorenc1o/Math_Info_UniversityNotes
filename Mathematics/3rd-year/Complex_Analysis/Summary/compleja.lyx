\begin_layout Standard
Si 
\begin_inset Formula $\boldsymbol{a}$
\end_inset

 
\series bold
es regular
\series default
, entonces 
\begin_inset Formula $\exists g\in\mathcal{H}(\Omega),g(z)=f(z),\forall z\neq a\implies Res(f,a)=\frac{1}{2\pi i}\int_{C(a,\rho)}f(z)dz=\frac{1}{2\pi i}\int_{C(a,\rho)}g(z)dz\overset{g\in\mathcal{H}(\Omega)}{=}0$
\end_inset


\end_layout

\begin_layout Standard
Si 
\begin_inset Formula $\boldsymbol{a}$
\end_inset

 
\series bold
es un polo de orden 
\begin_inset Formula $\boldsymbol{k}$
\end_inset


\series default
, entonces 
\begin_inset Formula $\exists g\in\mathcal{H}(\Omega):g(z)=(z-a)^{k}f(z),\forall z\neq a$
\end_inset

 donde ahora 
\begin_inset Formula $a$
\end_inset

 es regular para 
\begin_inset Formula $g$
\end_inset

.
 Así
\begin_inset Formula 
\[
Res(f,a)=\frac{1}{2\pi i}\int_{C(a,\rho)}f(z)dz=\frac{1}{2\pi i}\int_{C(a,\rho)}\frac{g(z)}{(z-a)^{k}}dz\overset{trm\,Taylor}{=}\frac{g^{k-1)}(a)}{(k-1)!}=
\]

\end_inset


\begin_inset Formula 
\[
=\lim_{z\rightarrow a}\frac{g^{k-1)}(z)}{(k-1)!}=\lim_{z\rightarrow a}\frac{\frac{d^{k-1}}{dz^{k-1}}((z-a)^{k}f(z))}{(k-1)!}
\]

\end_inset


\end_layout

\begin_layout Standard
Si 
\begin_inset Formula $\boldsymbol{k=1}$
\end_inset

, entonces 
\begin_inset Formula $Res(f,a)=\lim_{z\rightarrow a}(z-a)f(z)$
\end_inset


\end_layout

\begin_layout Standard
Y en el caso particular en el que 
\begin_inset Formula $\boldsymbol{f(z)=\frac{P(z)}{Q(z)}}$
\end_inset

 es una función racional, con 
\begin_inset Formula $P(a)\neq0$
\end_inset

 y 
\begin_inset Formula $a$
\end_inset

 es un cero simple de 
\begin_inset Formula $Q(z)$
\end_inset

 (
\begin_inset Formula $Q(a)=0$
\end_inset

 pero 
\begin_inset Formula $Q'(a)\neq0$
\end_inset

).
 Entonces
\begin_inset Formula 
\[
f(z)=\frac{P(z)}{Q(z)}=\frac{P(z)}{(z-a)g(z)}\implies Res(f,a)=\frac{P(a)}{g(a)}\overset{*}{=}\frac{P(a)}{Q'(a)}
\]

\end_inset


\end_layout

\begin_layout Standard
*
\begin_inset Formula 
\[
Q(z)=\sum_{n=0}^{\infty}c_{n}(z-a)^{n}\overset{c_{0}=Q(a)=0}{=}\sum_{n=1}^{\infty}c_{n}(z-a)^{n}=(z-a)\sum_{n=1}^{\infty}c_{n}(z-a)^{n-1}=
\]

\end_inset


\begin_inset Formula 
\[
=(z-a)\sum_{n=0}^{\infty}c_{n+1}(z-a)^{n}
\]

\end_inset


\end_layout

\begin_layout Standard
Y queda que 
\begin_inset Formula $g(z)=\sum_{n=0}^{\infty}c_{n+1}(z-a)^{n}$
\end_inset

, pero 
\begin_inset Formula $g(a)=c_{1}=Q'(a)$
\end_inset

.
\end_layout

