status open

\begin_layout Theorem

\series bold
Cauchy-Goursat
\end_layout

\begin_layout Theorem
Sea 
\begin_inset Formula $f\in\mathcal{H}(\Omega)$
\end_inset

 y 
\begin_inset Formula $\Delta(a,b,c)=\{z=ra+sb+tc:r,s,t\geq0,r+s+t=1\}\subset\Omega$
\end_inset

.
\end_layout

\begin_layout Theorem
Entonces
\begin_inset Formula 
\[
\int_{[a,b,c,a]}f(z)dz=0
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Proof
Sea 
\begin_inset Formula 
\[
I=\int_{[a,b,c,a]}f(z)dz
\]

\end_inset

 ¿Tendremos 
\begin_inset Formula $I=0?$
\end_inset

 
\end_layout

\begin_layout Proof
Sean ahora 
\begin_inset Formula 
\[
a'=\frac{c+b}{2}\quad b'=\frac{a+c}{2}\quad c'=\frac{a+b}{2}
\]

\end_inset


\end_layout

\begin_layout Proof
Entonces, por la linealidad de la integral respecto al camino:
\begin_inset Formula 
\[
I=\int_{[a,b,c,a]}f(z)dz=\int_{[a,c',b',a]}f(z)dz+\int_{[b',a',c,b']}f(z)dz+\int_{[c',b,a',c']}f(z)dz+\int_{[b',c',a',b']}f(z)dz
\]

\end_inset


\end_layout

\begin_layout Proof
Tomamos la integral que quede mayor en módulo:
\begin_inset Formula 
\[
I_{1}=\int_{[a_{1},b_{1},c_{1},a_{1}]}f(z)dz
\]

\end_inset


\end_layout

\begin_layout Proof
Así:
\begin_inset Formula 
\[
|I|\leq4|I_{1}|
\]

\end_inset


\end_layout

\begin_layout Proof
Si llamamos:
\begin_inset Formula 
\[
[a_{1},b_{1},c_{1},a_{1}]=\gamma_{1}
\]

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
\Delta(a_{1},b_{1},c_{1})=\Delta_{1}
\]

\end_inset


\end_layout

\begin_layout Proof
Entonces 
\begin_inset Formula 
\[
I_{1}=\int_{\gamma_{1}}fdz
\]

\end_inset


\end_layout

\begin_layout Proof
Y se tiene que 
\begin_inset Formula $diam(\Delta_{1})=\frac{1}{2}diam(\Delta)$
\end_inset

 y que 
\begin_inset Formula $l(\gamma_{1})=\frac{1}{2}l(\gamma)$
\end_inset

.
\end_layout

\begin_layout Proof
Prosiguiendo de forma inductiva, obtenemos 
\begin_inset Formula 
\[
\Delta_{n},\gamma_{n},I_{n}=\int_{\gamma_{n}}f(z)dz
\]

\end_inset


\end_layout

\begin_layout Proof
Con 
\begin_inset Formula 
\[
|I_{n-1}|\leq4|I_{n}|,\quad diam(\Delta_{n})=\frac{1}{2}diam(\Delta_{n-1}),\quad l(\gamma_{n})=\frac{1}{2}l(\gamma_{n-1})
\]

\end_inset


\end_layout

\begin_layout Proof
Por tanto
\begin_inset Formula 
\[
|I|=|I_{0}|\leq4^{n}|I_{n}|,\qquad diam(\Delta_{n})=\frac{1}{2^{n}}diam(\Delta),\qquad l(\gamma_{n})=\frac{1}{2^{n}}l(\gamma_{0})
\]

\end_inset


\end_layout

\begin_layout Proof
Ahora, como 
\begin_inset Formula $\Delta_{n}\subsetneq\Delta_{n-1},\forall n\in\mathbb{N}$
\end_inset

, 
\begin_inset Formula $diam(\Delta_{n})\rightarrow0$
\end_inset

 y 
\begin_inset Formula $\Delta_{n}$
\end_inset

 es compacto, 
\begin_inset Formula $\forall n$
\end_inset

, entonces 
\begin_inset Formula $\exists\alpha$
\end_inset

 tal que
\begin_inset Formula 
\[
\cap_{n=0}^{\infty}\Delta_{n}=\{\alpha\}
\]

\end_inset


\end_layout

\begin_layout Proof
Como 
\begin_inset Formula $f$
\end_inset

 es holomorfa en 
\begin_inset Formula $\alpha$
\end_inset

, dado 
\begin_inset Formula $\varepsilon>0,\exists\delta>0:|z-\alpha|\leq\delta\implies|\frac{f(z)-f(\alpha)}{z-\alpha}-f'(\alpha)|\leq\varepsilon\iff|f(z)-f(\alpha)-f'(\alpha)|z-\alpha||\leq\varepsilon|z-\alpha|$
\end_inset


\end_layout

\begin_layout Proof
Sea 
\begin_inset Formula $n_{0}/\Delta_{n_{0}}\subset\overline{D}(\alpha,\delta)$
\end_inset

, entonces:
\begin_inset Formula 
\[
|I|\leq4^{n_{0}}|I_{n_{0}}|=4^{n_{0}}|\int_{\gamma_{n_{0}}}fdz|\overset{*}{=}4^{n_{0}}|\int_{\gamma_{n_{0}}}(f(z)-f(\alpha)-f'(\alpha)(z-\alpha))dz|\leq4^{n_{0}}M\cdot l(\gamma_{n_{0}})
\]

\end_inset


\end_layout

\begin_layout Proof
Donde:
\end_layout

\begin_layout Proof
* es cierto porque 
\begin_inset Formula $\gamma_{n_{0}}$
\end_inset

 es un camino cerrado y 
\begin_inset Formula $f(\alpha)+f'(\alpha)(z-\alpha)$
\end_inset

 es un polinomio, por lo que tiene primitiva, y entonces
\begin_inset Formula 
\[
\int_{\gamma_{n_{0}}}f(\alpha)+f'(\alpha)(z-\alpha)dz=0
\]

\end_inset


\end_layout

\begin_layout Proof
Y 
\begin_inset Formula $M=\max_{z\in\gamma_{n_{0}}^{*}\subset\overline{D}(\alpha,\delta)}\{|f(z)-f(\alpha)-f'(\alpha)(z-\alpha)|\}\leq\max_{z\in\gamma_{n_{0}}}\{\varepsilon|z-\alpha|\}\leq\varepsilon\cdot diam(\Delta_{n_{0}})$
\end_inset


\end_layout

\begin_layout Proof
Y así:
\begin_inset Formula 
\[
|I|\leq4^{n_{0}}M\cdot l(\gamma_{n_{0}})\leq4^{n_{0}}\varepsilon\cdot diam(\Delta_{n_{0}})l(\gamma_{n_{0}})=4^{n_{0}}\varepsilon\frac{1}{2^{n_{0}}}diam(\Delta_{0})\frac{1}{2^{n_{0}}}l(\gamma_{0})=\varepsilon\cdot diam(\Delta_{0})\cdot l(\gamma_{0})
\]

\end_inset


\end_layout

\begin_layout Proof
Por lo que
\begin_inset Formula 
\[
|I|\leq\varepsilon,\forall\varepsilon>0\implies|I|=0
\]

\end_inset


\end_layout

