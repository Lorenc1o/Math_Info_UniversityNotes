 que 
\begin_inset Formula $\int_{C(a,r)}\frac{1}{w-z}dw=0$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
\int_{C(a,r)}\frac{1}{w-z}dw=\int_{C(a,r)}\frac{1}{w-a-(z-a)}dw=\int_{C(a,r)}\frac{1}{w-a}\frac{1}{1-\frac{z-a}{w-a}}dw
\]

\end_inset


\end_layout

\begin_layout Standard
Donde 
\begin_inset Formula $\frac{1}{1-\frac{z-a}{w-a}}$
\end_inset

 podemos verlo como la suma de la progresión geométrica de ratio 
\begin_inset Formula $\frac{z-a}{w-a}<1$
\end_inset

, así:
\begin_inset Formula 
\[
\int_{C(a,r)}\frac{1}{w-z}dw=\int_{C(a,r)}\frac{1}{w-a}\sum_{n=0}^{\infty}(\frac{z-a}{w-a})^{n}dw=\int_{C(a,r)}\sum_{n=0}^{\infty}\frac{(z-a)^{n}}{(w-a)^{n+1}}dw\overset{f_{n}(w)=\frac{(z-a)^{n}}{(w-a)^{n+1}}}{=}
\]

\end_inset


\begin_inset Formula 
\[
=\int_{C(a,r)}\sum_{n=0}^{\infty}f_{n}(w)dw\overset{*}{=}\sum_{n=0}^{\infty}\int_{C(a,r)}f_{n}(w)dw=\sum_{n=0}^{\infty}\int_{C(a,r)}\frac{(z-a)^{n}}{(w-a)^{n+1}}dw=
\]

\end_inset


\begin_inset Formula 
\[
=\sum_{n=0}^{\infty}(z-a)^{n}\int_{C(a,r)}\frac{1}{(w-a)^{n+1}}dw\overset{**}{=}\int_{C(a,r)}\frac{1}{w-a}dw=\int_{-\pi}^{\pi}\frac{1}{a+re^{it}-a}rie^{it}dt=
\]

\end_inset


\begin_inset Formula 
\[
=\int_{-\pi}^{\pi}idt=2\pi i
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Proof
*: 
\begin_inset Formula 
\[
\forall w\in C(a,r)^{*},\sum_{n=0}^{\infty}|f_{n}(w)|\leq\sum_{n=0}^{\infty}\frac{|z-a|^{n}}{r^{n+1}}=\frac{1}{r}\sum_{n=0}^{\infty}\frac{|z-a|^{n}}{r^{n}}\overset{\frac{z-a}{r}<1}{<}\infty
\]

\end_inset


\end_layout

\begin_layout Proof
Y así, 
\begin_inset Formula $f_{n}$
