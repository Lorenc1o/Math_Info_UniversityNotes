\begin_layout Theorem

\series bold
Regla de L'hôpital
\end_layout

\begin_layout Theorem
Sean 
\begin_inset Formula $f,g$
\end_inset

 no identicamente nulas cerca de 
\begin_inset Formula $a$
\end_inset

, con 
\begin_inset Formula $f,g\in\mathcal{H}(D(a,\varepsilon))$
\end_inset

.
 Con 
\begin_inset Formula $f(a)=g(a)=0$
\end_inset

.
 Entonces
\begin_inset Formula 
\[
\lim_{z\rightarrow a}\frac{f(z)}{g(z)}=\lim_{z\rightarrow a}\frac{f'(z)}{g'(z)}
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Proof
Supongamos que 
\begin_inset Formula $a$
\end_inset

 es un cero de orden 
\begin_inset Formula $k\geq1$
\end_inset

 de 
\begin_inset Formula $f\implies f(z)=(z-a)^{k}F(z),\ F(a)\neq0\ y\ F\in\mathcal{H}(D(a,r))$
\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula $a$
\end_inset

 cero de orden 
\begin_inset Formula $l\geq1$
\end_inset

 de 
\begin_inset Formula $g\implies g(z)=(z-a)^{l}G(z),\ G(a)\neq0\ y\ F\in\mathcal{H}(D(a,r'))$
\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
\lim_{z\rightarrow a}\frac{f(z)}{g(z)}=\lim_{z\rightarrow a}\frac{(z-a)^{k}}{(z-a)^{l}}\frac{F(z)}{G(z)}=\lim_{z\rightarrow a}(z-a)^{k-l}\frac{F(z)}{G(z)}=\begin{cases}
0 & k>l\\
\frac{F(a)}{G(a)} & k=l\\
\infty & k<l
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Proof
Y, por otro lado
\begin_inset Formula 
\[
\lim_{z\rightarrow a}\frac{f'(z)}{g'(z)}=\lim_{z\rightarrow a}\frac{(z-a)^{k}F'(z)+k(z-a)^{k-1}F(z)}{(z-a)^{l}G'(z)+l(z-a)^{l-1}G(z)}=\lim_{z\rightarrow a}\frac{(z-a)^{k-1}}{(z-a)^{l-1}}\frac{(z-a)F'(z)+kF(z)}{(z-a)G'(z)+lG(z)}=
\]

\end_inset


\begin_inset Formula 
\[
=\lim_{z\rightarrow a}(z-a)^{k-l}\frac{(z-a)F'(z)+kF(z)}{(z-a)G'(z)+lG(z)}\overset{*}{=}\begin{cases}
0 & k>l\\
\frac{F(a)}{G(a)} & k=l\\
\infty & k<l
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Proof
*: Pues 
\begin_inset Formula 
\[
\lim_{z\rightarrow a}(z-a)F'(z)=0=\lim_{z\rightarrow a}(z-a)G'(z)
\]

\end_inset


\end_layout

\begin_layout Proof
Y vemos así como ambos límites coinciden.
\end_layout

